  "type": "module",
  "description": "Slack Message Manager",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "engines": {
    "node": "^14.13 || >=16.0.0"
  },
  "scripts": {
    "build": "rm -rf lib && tsc --build tsconfig.build.json",
