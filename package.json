    "@types/react": "17.0.47",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@okitan/ink-slack": "^2.0.0",
    "@slack/web-api": "^6.1.0",
