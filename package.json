  "devDependencies": {
    "@types/react": "18.2.15",
    "prettier": "3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@okitan/ink-slack": "^2.1.0",
    "@slack/web-api": "^6.1.0",
    "async": "^3.2.0",
    "zen-observable": "^0.10.0"
